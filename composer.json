{
	"name": "coderscantina/hashidable",
	"type": "library",
	"description": "An adapted bridge for using hashids in Laravel models.",
	"keywords": [
		"laravel",
		"laravel-hashids",
		"hashids"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Michael Wallner",
			"email": "m.wallner@badsoft.net"
		}
	],
	"require": {
		"php": "~7.3||^8.0",
		"illuminate/support": "^8.0||^9.0||^10.0||^11.0",
		"vinkla/hashids": "~9.1||~10.0||^11.0||^12.0"
	},
	"require-dev": {
		"illuminate/database": "^8.0||^9.0||^10.0||^11.0",
		"graham-campbell/testbench": "^5.4||^6.0",
		"mockery/mockery": "^1.3",
		"phpunit/phpunit": "^9.3"
	},
	"autoload": {
		"psr-4": {
			"CodersCantina\\Hashids\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodersCantina\\Hashids\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
